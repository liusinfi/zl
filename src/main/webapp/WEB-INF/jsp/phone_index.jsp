<script>
    $("#md-close-trademark").click(function () {
        if(!$("#J-chance-success").hasClass("ipr-hide")){
            window.location.reload();
        }
        $("#J-chance").removeClass("visible");
        $("#J-chance-child").removeClass("visible");
        $("#J-chance").addClass("ipr-hide");
        $("#J-chance-child").addClass("ipr-hide");
    });
    $('#shipeibtn').click(function () {
        showmes();
    });
    $('#manzubtn').click(function () {
        showmes();
    });
    $('#servicebtn').click(function () {
        showmes();
    });
    $('#zixunbtn').click(function () {
        showmes();
    });
    function reloadpage() {
        window.location.reload();
    }

    function submitquery() {
        var desc = $("#J-chance-name").val();
        var mobile = $("#J-chance-mobile").val();
        if(!validata(desc,mobile)){
            return;
        }
        requestAjaxQuery(desc,mobile);
    }

    function showmes() {
        if(!$("#J-chance").hasClass("visible")){
            $("#J-chance").addClass("visible");
        }
        if(!$("#J-chance-child").hasClass("visible")){
            $("#J-chance-child").addClass("visible");
        }
        if($("#J-chance").hasClass("ipr-hide")){
            $("#J-chance").removeClass("ipr-hide");
        }
        if($("#J-chance-child").hasClass("ipr-hide")){
            $("#J-chance-child").removeClass("ipr-hide");
        }
    }
    function firstBtn() {
        var desc = $("#descdesc").val();
        var mobile = $("#mobilemobile").val();
        if(!validata(desc,mobile)){
            return;
        }
        requestAjaxQuery(desc,mobile);
    }
    function validata(desc,mobile) {
        if(mobile==null || mobile==""){
            $("#weuimsg").html("手机号不能为空");
            $("#validmobile").addClass("weui_toast_visible");
            setTimeout(function () {
                $("#validmobile").removeClass("weui_toast_visible");
            },1500)
            return false;
        }
        if(!checkMobile(mobile)){
            $("#weuimsg").html("请输入正确手机号码");
            $("#validmobile").addClass("weui_toast_visible");
            setTimeout(function () {
                $("#validmobile").removeClass("weui_toast_visible");
            },1500)
            return false;
        }
        return true;
    }
    function checkMobile(str) {
        var reg = /^1[0-9]{10}$/;
        if (reg.test(str)) {
            return true;
        } else {
            return false;
        }
    }
    function requestAjaxQuery(desc,mobile) {
        $.ajax({
            url:"/ajaxQueryZl",
            type:"POST",
            data:{'desc':desc,'mobile':mobile},
            success:function(result){
                showmes();
                $("#J-chance-success").removeClass("ipr-hide");
                $("#J-chance-submit").addClass("ipr-hide");
            }
        });
    }
</script>

